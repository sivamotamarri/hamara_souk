<div class="top-block">

  <span class="login-links">
    <% if user_signed_in? %>
   
      <ul class="nav nav-pills">
        <li id="user" class ='dropdown'>
          <a class="dropdown-toggle" data-toggle="dropdown"  href="#user" ><span style="vertical-align: super;"><%= user_display_pic(current_user, '30x30')%></span><%=  current_user.full_name %><b class="caret"></b></a>
          <ul class="dropdown-menu">
            <li> <%= link_to("Edit Profile", edit_user_registration_path)%></li>
            <li><%= link_to("My Watchings", watchings_path) %></li>
            <li><%= link_to("My Ads", my_ads_path) %></li>
            <li><%= link_to("My Reviews", my_reviews_path) %></li>
            <li><%= link_to("My Profile", profile_path) %></li>
            <li><%= link_to("Sign out", destroy_user_session_path) %></li>
          </ul>
        </li>  
      </ul>
      
      
    
    <% else %>
        <%- if devise_mapping.omniauthable? %>
          <a href="<%=  omniauth_authorize_path(:user, :facebook) %>"><img src="/assets/fb.png" alt="Facebook" border="none" /></a>
       <% end -%>
       <%- if controller_name != 'sessions' %>          
          <%= link_to "Sign in" , '#signin_modal', "data-toggle" => "modal" %>
      <% end -%> |
      <%- if devise_mapping.registerable? && controller_name != 'registrations' %>
        <%= link_to "Register", new_registration_path(resource_name) %>
      <% end -%>
    <% end %>
  </span>
  
</div>
